          "id": 27,
          "content": "Preparing for Your Appointment",
          "key": 6,
          "children": [
            {
              "id": 28,
              "content": "",
              "isList": true,
              "key": 1,
              "children": [
                {
                  "id": 29,
                  "content": "On the day of your visit, please bring the following items:",
                  "key": 1
                },
                {
                  "id": 30,
                  "content": "Your insurance card, photo identification and payment for your co-pay",
                  "key": 2
                },
                {
                  "id": 31,
                  "content": "A list of your current medications. It is important for your primary care physician to know this information",
                  "key": 3
                }
              ]
            },
            {
              "id": 32,
              "content": "Hahnemann University Primary Care accepts most major types of insurance. If you are unsure as to whether or not your insurance will be accepted, please contact the office in which your appointment is scheduled.",
              "key": 2
            }
          ]
